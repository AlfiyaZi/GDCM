<ServerManagerConfiguration>
  <ProxyGroup name="sources">
    <!-- ================================================================== -->
    <SourceProxy name="PTXReader" class="vtkGDCMImageReader" label="GDCMImageReader">
      <Documentation
         long_help="Read a DICOM Image file."
         short_help="Read a DICOM Image file.">
      </Documentation>
      <StringVectorProperty
            name="FileName"
            animateable="0"
            command="SetFileName"
            number_of_elements="1">
        <FileListDomain name="files"/>
        <Documentation>
          This property specifies the file name for the GDCM reader.
        </Documentation>
      </StringVectorProperty>

      <Hints>
        <ReaderFactory extensions="dcm"
                       file_description="DICOM File Format" />
     </Hints>
    </SourceProxy>
    <!-- End PTXReader -->
  </ProxyGroup>
  <!-- End Filters Group -->
</ServerManagerConfiguration>
